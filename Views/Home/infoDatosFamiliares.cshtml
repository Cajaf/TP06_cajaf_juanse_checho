
<body>
<header>
<a href='@Url.Action("MostrarDatosPersonales","Home")'> Inicio</a>
<a href='@Url.Action("Deslogearse","Home")'> Deslogearse</a>
</header>
<main>
<h1>Datos familiares de @ViewBag.Nombre </h1>
 <ul>
 @foreach(DatoFamiliar item in ViewBag.List)
    {
        <li>Nombre: @item.Nombre Apellido: @item.Apellido Parentesco: @item.Parentesco</li>
    }
 </ul>
 </main>
</body>
 
    